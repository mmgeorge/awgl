

/** Defintiions for a 3x3 matrix */
class mat3 {
    constructor(){
	this.data = new Float32Array(9);
    }

    multiply_vec3(v){
	return vec3(
	    v.x*data[0] + v.y*data[1] + v.z*data[2],
	    v.x*data[3] + v.y*data[4] + v.z*data[5],
	    v.x*data[6] + v.y*data[7] + v.z*data[8]); 
    }
}

/** Defintions for a 4x4 matrix */
class mat4 {
    constructor(){
	this.data = new Float32Array(12); 
    }
}
